# PostgreSQL configuration
POSTGRES_USER=trainium_user
POSTGRES_PASSWORD=changePGpassword
POSTGRES_DB=trainium
POSTGRES_PORT=5434

# Service ports
POSTGREST_PORT=3000
FRONTEND_PORT=5173
PYTHON_SERVICE_PORT=8000

# Application settings
ENVIRONMENT=development
LOG_LEVEL=INFO
DEBUG=false

# API keys
GEMINI_API_KEY=your_gemini_api_key
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
HF_TOKEN=your_huggingface_api_key
AWS_ACCESS_KEY_ID=your_aws_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION_NAME=us-east-1

# LLM Configuration
# Preferred model for Ollama
LLM_MODEL=gpt-oss:20b
# Compose will build preferences using LLM_MODEL
LLM_PREFERENCE=ollama:gpt-oss:20b,openai:gpt-4o-mini,gemini:gemini-1.5-flash
OLLAMA_HOST=http://host.docker.internal:11434
OLLAMA_PORT=11434

# ChromaDB configuration
CHROMA_URL=chromadb
CHROMA_PORT=8000

# Embedding configuration
EMBEDDING_PROVIDER=openai
EMBEDDING_MODEL=text-embedding-3-small

# Redis configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0

# Database connection string  --UPDATE with values above
DATABASE_URL=postgresql://trainium_user:changePGpassword@db:5432/trainium
POSTGREST_URL=http://postgrest:3000
REDIS_URL=redis://redis:6379/0

# LinkedIn MCP Server configuration
LINKEDIN_EMAIL=your_linkedin_email@example.com
LINKEDIN_PASSWORD=your_linkedin_password
LINKEDIN_COOKIE=your_linkedin_session_cookie

# Job Poller configuration
POLL_INTERVAL_MINUTES=5
JOB_REVIEW_QUEUE_NAME=job_review

