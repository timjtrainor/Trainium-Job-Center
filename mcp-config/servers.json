{
  "servers": {
    "duckduckgo": {
      "name": "DuckDuckGo Search MCP Server",
      "description": "Provides web search capabilities using DuckDuckGo",
      "transport": "streaming",
      "endpoints": {
        "search": "/search",
        "tools": "/tools"
      },
      "capabilities": ["search", "web_lookup"],
      "status": "configured"
    },
    "linkedin-mcp-server": {
      "name": "LinkedIn MCP Server",
      "description": "Provides LinkedIn job search and networking capabilities",
      "transport": "streaming",
      "endpoints": {
        "job_search": "/jobs/search",
        "profile_lookup": "/profile",
        "tools": "/tools"
      },
      "capabilities": ["job_search", "profile_lookup", "networking"],
      "status": "configured"
    }
  },
  "gateway_config": {
    "protocol_version": "2025-03-26",
    "transport": "streaming",
    "timeout": 30,
    "max_retries": 3,
    "log_level": "INFO"
  }
}