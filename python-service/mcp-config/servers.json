{
  "servers": {
    "duckduckgo": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "mcp/duckduckgo"],
      "env": {},
      "description": "DuckDuckGo search server for web searches"
    },
    "linkedin-mcp-serves": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "-e", "LINKEDIN_COOKIE","stickerdaniel/linkedin-mcp-server"],
      "env": {
        "LINKEDIN_COOKIE": "${LINKEDIN_COOKIE}"
      },
      "description": "LinkedIn MCP server for LinkedIn operations and data access"
    }
  },
  "gateway": {
    "port": 8811,
    "host": "0.0.0.0"
  }
}
